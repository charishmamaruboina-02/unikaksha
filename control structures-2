#series-1
n=int(input())
for i in range(1,n+1):
    print(i**2,end=' ')
   
   

#series-2
 n = int(input())
sum = 6
for i in range(1,n+1):
    print(sum,end=" ")
    sum = sum + i * 5
  
  
